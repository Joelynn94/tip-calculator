  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/runtime-corejs2": "^7.15.3",
    "parcel-bundler": "^1.12.5",
    "sass": "^1.38.1"
  }
}
